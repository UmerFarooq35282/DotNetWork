@model WebApplication2.Models.Orders;
@{
    ViewData["Title"] = "About";
}

<h1>@ViewData["Title"]</h1>
<p>@ViewData["Message"]</p>

<table class="table">
    <thead>
    <th class="col">Name</th>
    <th class="col">Description</th>
    <th class="col">Address</th>
    </thead>
    <tbody>
    <td>@Model.Name</td>
    <td>@Model.Description</td>
    <td>@Model.Address</td>
    </tbody>
</table>
